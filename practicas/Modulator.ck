public class Modulator
{
    fun void close(float duration)
    {
        Synth synth;
        while(true){
            for(100 => int i; i < 10000; i++)
            {
                i => synth.modulatorGain;
                //<<< "Modulator:modulation:", i>>>;
                1*duration::ms => now;
            }
        }
    }
}
